Enter a number: 4
ABCD
ABCD
ABCD
ABCD
//code

int main()
 {
    int n;
    cout<<"Enter a number: ";
    cin>>n; 
    for(int i=0;i<n;i++)
    {
        char ch='A';
        for(int j=0;j<n;j++)
        {
            cout<<ch;
            ch=ch+1;
        }
        cout<<endl;
    }
 }

