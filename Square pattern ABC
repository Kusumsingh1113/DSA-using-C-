int main()
{
    int n,i,j;
    cout<<"Enter n: ";
    cin>>n;
    char ch='A';
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            cout<<ch;
            ch=ch+1;
        }
        cout<<endl;
    }
}

